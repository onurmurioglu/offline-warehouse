<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Products"
		items="{listProducts>/}"
		search="onSearchProducts"
		confirm="productConfirm"
		cancel="">
		<StandardListItem
			icon="{listProducts>image}"
			iconDensityAware="false"
			iconInset="false"
			title="{listProducts>title}"
			description="Stock Quentity: {listProducts>stock}" />
	</SelectDialog>
</core:FragmentDefinition>